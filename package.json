{
  "name": "we-transfer-tech-test",
  "version": "1.0.0",
  "description": "We Transfer Tech Test",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc -b",
    "clean": "rimraf ./reporting/json/* ./reporting/data.js errorScreenshots ./specs/step-definitions/test-data/download/* ./downloads/*",
    "clean:docker": "rimraf ./reporting/json/* ./reporting/data.js ./specs/step-definitions/test-data/download/* ./downloads/*",
    "eslint": "npx eslint ./lib/**/* ./page-objects/**/* ./specs/**/*.ts",
    "eslint-fix": "npx eslint ./lib/**/* ./page-objects/**/* ./specs/**/*.ts --fix",
    "selenium-standalone": "npx selenium-standalone install --config=./selenium-webdriver-versions.json",
    "prechrome:docker": "failsafe clean:docker build eslint",
    "chrome:docker": "wdio ./config/wdio.chrome.js",
    "prechrome": "failsafe clean build eslint selenium-standalone",
    "chrome": "wdio ./config/wdio.chrome.js",
    "docker:base-container": "docker build -f Dockerfile.linux.environment --tag=tech-test-base .",
    "predocker:build": "failsafe eslint clean:docker docker:base-container",
    "docker:build": "docker build --tag=tech-tests .",
    "docker:test": "docker-compose run --rm tests"
  },
  "keywords": [
    "WebdriverIO",
    "NodeJS",
    "Cucumber"
  ],
  "author": "Abdul Farhat",
  "license": "ISC",
  "dependencies": {
    "@types/cucumber": "^6.0.1",
    "@types/node": "^14.11.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@wdio/cli": "^6.6.0",
    "@wdio/cucumber-framework": "^6.6.0",
    "@wdio/junit-reporter": "^6.6.0",
    "@wdio/local-runner": "^6.6.0",
    "@wdio/selenium-standalone-service": "^6.6.0",
    "@wdio/spec-reporter": "^6.6.0",
    "@wdio/sync": "^6.6.0",
    "combine-json": "^1.0.0",
    "eslint": "^7.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-cucumber": "^1.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-wdio": "^6.6.0",
    "expect-webdriverio": "^1.4.1",
    "npm-failsafe": "^0.4.1",
    "sha256-file": "^1.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "wdio-cucumberjs-json-reporter": "^2.0.2",
    "webdriverio": "^6.6.0"
  },
  "devDependencies": {}
}
